{
  "name": "rmk",
  "version": "2.2.1",
  "main": "dist/rmk.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rollup -c",
    "test": "jest",
    "test-dev": "jest --coverage --watch",
    "doc": "jsdoc --configure .jsdoc.json",
    "style": "jscs lib/*.js lib/**/*.js",
    "jsdoc2md:actions": "jsdoc2md lib/actions/*.js  lib/actions/**/*.js > docs/ACTIONS.md",
    "jsdoc2md:rmk": "jsdoc2md lib/index.js > docs/README.md",
    "jsdoc2md": "npm run jsdoc2md:actions",
    "github": "npm run jsdoc2md && git commit -a -m \"generate docs\" && git push",
    "format-backend": "prettier --trailing-comma es5 --single-quote --write 'lib/**/*.js' '__tests__/**/*.js'",
    "pref": "npm run build && babel-node tasks/bestResults.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "chalk": "^1.1.3",
    "cross-spawn": "^5.1.0",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "jsdoc": "^3.4.3",
    "jsdoc-to-markdown": "^3.0.0",
    "lint-staged": "^3.4.2",
    "prettier": "^0.22.0",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-minify": "^1.0.3",
    "rollup-plugin-node-resolve": "^3.0.0",
    "yargs": "^7.0.2"
  },
  "dependencies": {}
}
