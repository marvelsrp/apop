{
  "name": "rmk",
  "version": "1.0.0",
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel -d ./ ./lib",
    "dev": "babel -d ./dist ./lib --watch",
    "test": "jest --coverage",
    "test-dev": "jest --coverage --watch",
    "doc": "jsdoc --configure .jsdoc.json",
    "style": "jscs lib/*.js lib/**/*.js",
    "jsdoc2md:actions": "jsdoc2md lib/actions/*.js  lib/actions/**/*.js > lib/actions/README.md",
    "jsdoc2md:shortcuts": "jsdoc2md lib/shortcuts/*.js  lib/shortcuts/**/*.js > lib/shortcuts/README.md",
    "jsdoc2md:utils": "jsdoc2md lib/utils/*.js  lib/utils/**/*.js > lib/utils/README.md",
    "jsdoc2md": "npm run jsdoc2md:actions && npm run jsdoc2md:shortcuts && npm run jsdoc2md:utils",
    "github": "npm run jsdoc2md && git commit -a -m \"generate docs\" && git push",
    "eslint": "eslint ./lib ./__tests__",
    "eslint:fix": "eslint ./lib ./__tests__ --fix"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "jest": "^18.1.0",
    "jsdoc": "^3.4.3",
    "jsdoc-to-markdown": "^3.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
